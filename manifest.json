{
    "manifest_version": 2,

  "name": "Vinno",
  "description": "Video annotation tool",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
      "persistent": false,
      "page": "vinno.html"
    },
  "content_scripts": [ {
    "js": [ "jquery.min.js"],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "permissions": [
    "tabs"
  ]
}