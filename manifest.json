{
    "manifest_version": 2,

    "name": "Vinno",
    "description": "Video annotation tool",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["vinno.js"],
        "persistent": false
    },
    "content_scripts": [ {
        "js": [ "jquery.min.js"],
        "matches": [ "http://*/*", "https://*/*"]
    }]
}