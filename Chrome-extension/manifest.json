{
    "manifest_version": 2,

    "name": "Vinno",
    "description": "Video annotation tool",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup":"vinno.html"
    },
    "content_scripts": [ {
        "js": [ "jquery.min.js","contentScript.js"],
        "matches": [ "http://*/*", "https://*/*"]
    }],
    "background":{
    "scripts": ["jquery.min.js","backgroundScript.js"]
    },
    "permissions": [
        "tabs"
    ]
}